@model E_voting_System.Models.ElectionSchedule

@{
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";

}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <h4>ElectionSchedule</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.ID)
 



        <div class="form-group">
            تاريخ بدء الانتخابات            <div class="col-md-10">
                @Html.EditorFor(model => model.Start_Date_Of_Election, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Start_Date_Of_Election, "", new { @class = "text-danger" })
            </div>
        </div>

  
        <div class="form-group">
            تاريخ نهاية الانتخابات            <div class="col-md-10">
                @Html.EditorFor(model => model.End_Date_Of_Election, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.End_Date_Of_Election, "", new { @class = "text-danger" })
            </div>
        </div>



        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="حفظ" class="btn btn-default" />
            </div>
        </div>
    </div>
}